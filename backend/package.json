{
    "name": "backend",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "typeorm": "typeorm-ts-node-commonjs",
        "db:test": "ts-node src/scripts/test-db-connection.ts",
        "db:migrate": "npm run typeorm migration:run -- -d src/config/database.config.ts",
        "db:migrate:revert": "npm run typeorm migration:revert -- -d src/config/database.config.ts",
        "db:generate": "npm run typeorm migration:generate -- -d src/config/database.config.ts",
        "migration:generate": "npm run typeorm migration:generate",
        "migration:run": "npm run typeorm migration:run -- -d src/config/database.ts",
        "migration:revert": "npm run typeorm migration:revert -- -d src/config/database.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.16",
        "@types/socket.io": "^3.0.1",
        "@types/winston": "^2.4.4",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.2",
        "socket.io": "^4.8.1",
        "ts-node-dev": "^2.0.0",
        "typeorm": "^0.3.20",
        "typescript": "^5.3.3",
        "winston": "^3.17.0"
    },
    "devDependencies": {
