        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/pg": "^8.11.11",
        "@types/socket.io-client": "^1.4.36",
        "@types/supertest": "^6.0.2",
        "dotenv": "^16.4.7",
        "jest": "^29.7.0",
